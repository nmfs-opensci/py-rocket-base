{
    "ContentsManager": {
        "allow_hidden": true
    },
    "@jupyterlab/docmanager-extension:plugin": {
        "defaultViewers": {
            "qmd": "editor",
            "quarto": "editor"
        }
    },
    "@jupyterlab/docregistry-extension:plugin": {
        "fileTypes": [
            {
                "name": "qmd",
                "displayName": "Quarto Markdown",
                "extensions": [".qmd"],
                "mimeTypes": ["text/markdown"],
                "fileFormat": "text"
            }
        ]
    }
}
